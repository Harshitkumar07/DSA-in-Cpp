** Problem :- **



** Code:- **

class Solution {
  public:
    bool twoSum(vector<int>& arr, int target) {
        unordered_map<int, int> map;
        for (int i = 0; i < arr.size(); i++) {
            int first = arr[i], second = target - first;
            if (map.find(second) != map.end()) {
                return true;
            }
            map[first] = i;
        }
        return false;
    }
};

