** Problem :- **



** Code :- **

class Solution {
  public:
    int getSecondLargest(vector<int> &arr) {
        int largest = INT_MIN;
        int secLargest = INT_MIN;
        for (int num : arr) {
            if (num > largest) {
                secLargest = largest;
                largest = num;
            } else if (num > secLargest && num < largest) {
                secLargest = num;
            }
        }
        return (secLargest == INT_MIN) ? -1 : secLargest;
    }
};

