** Problem :- **



** Code :- **

class Solution {
  public:
    void segregateElements(vector<int>& arr) {
    int n = arr.size(),pos= 0;
    vector<int> ans(n);
    for (int i = 0; i < n; i++) {
        if (arr[i] >= 0) {
            ans[pos++] = arr[i];
        }
    }
    for (int i = 0; i < n; i++) {
        if (arr[i] < 0) {
            ans[pos++] = arr[i];
        }
    }
    arr = ans;
    }
};

