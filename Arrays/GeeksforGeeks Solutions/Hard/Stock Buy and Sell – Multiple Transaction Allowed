** Problem :- **



** Code :- **


class Solution {
  public:
    int maximumProfit(vector<int> &prices) {
        int min = INT_MAX, maxProfit = 0;
        for (int i = 1; i < prices.size(); i++) {
            if (prices[i] > prices[i - 1]) {
                maxProfit = maxProfit + (prices[i] - prices[i - 1]);
            }
        }
        return maxProfit;
    }
};

