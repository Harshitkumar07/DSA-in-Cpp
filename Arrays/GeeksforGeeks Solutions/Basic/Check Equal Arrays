** Problem :- **



** Code :- **

class Solution {
  public:
    // Function to check if two arrays are equal or not.
    bool check(vector<int>& arr1, vector<int>& arr2) {
        sort(arr1.begin(),arr1.end());
        sort(arr2.begin(),arr2.end());
        int ans = 0;
        for(int i=0;i<arr1.size();i++) {
            if(arr1[i]==arr2[i]) {
                ans++;
            }
        } 
        if(ans == arr1.size()) {
            return true;
        }
        return false;
    }
};

