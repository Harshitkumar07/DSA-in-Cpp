#include<iostream>
using namespace std;

int LengthOfString(string s) {
    if (s == "")
        return 0;
    return 1 + LengthOfString(s.substr(1));
}

int main() {
    string s;
    cout << "Enter a string: ";
    cin >> s;
    cout << "Length of string: " << LengthOfString(s) << endl;
    return 0;
}
